import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		int a[] = new int [10];
		
		for(int i=0;i<10;i++) {
		a[i] = in.nextInt();
		}
		
		int b = in.nextInt();
		int sum = 0;
		for(int k=0;k<10;k++) {
			if(b+30>=a[k]) {
				sum++;
			}
		}
		System.out.println(sum);
	}

}
